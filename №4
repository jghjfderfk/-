#include <iostream>
#include<math.h>
using namespace std;
int main()
{
	int a, b, S, P;
	cin >> a >> b;
	S = a * b;
	P = 2 * (a + b);
	cout << S << " " << P;
	return 0;
}

#include <iostream>
#include<math.h>
using namespace std;
int main()
{
	double L, π = 3.14;
	int d;
	cin >> d;
	L = π * d;
	cout << L;
	return 0;
}

#include <iostream>
#include<math.h>
using namespace std;
int main()
{
	int a, b;
	double S;
	cin >> a >> b;
	S = (double)(a + b) / 2;
	cout << S;
	return 0;
}

#include <iostream>
#include<math.h>
using namespace std;
int main()
{
	int a, b;
	double S, R, P, F;
	cin >> a >> b;
	S = a + b;
	R = a - b;
	P = a * b;
	F = (double)(a * a) / (b * b);
	cout << S << " " << R << " " << P << " " << F;
	return 0;
}

#include <iostream>
#include<math.h>
using namespace std;
int main()
{
	int a, b;
	double S, R, P, F;
	cin >> a >> b;
	S = a + b;
	R = a - b;
	P = a * b;
	F = (double)abs(a) / abs(b);
	cout << S << " " << R << " " << P << " " << F;
	return 0;
}
