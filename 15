#include <iostream>
using namespace std;
void PowerA3(double A, double& B)
{
    B = A * A * A;
}
int main(void)
{
    double arr[5] = { 1, 2, 3, 4, 5 };
    for (int i = 0; i < 5; i++)
    {
        PowerA3(arr[i], arr[i]);
        cout << arr[i] << ' ';
    }
    system("pause");
    return 0;
}

#include <iostream>
using namespace std;
int Sign(double x)
{
    return x < 0 ? -1 : ((x == 0) ? 0 : 1);
}
int main()
{
    double A, B;
    cout << "Vvedite A" << endl;
    cin >> A;
    cout << "Vvedite B" << endl;
    cin >> B;
    cout << Sign(A) + Sign(B) << endl;
    return 0;
}

#include <iostream>
using namespace std;
double RingS(double R1, double R2) {
	double S = 3.14 * (R1 * R1 - R2 * R2);
	if (R1 > R2) {
		return S;
	}
	else {
		return 0;
	}
}
int main() {
	for (int i = 1; i <= 3; i++) {
		double R1, R2;
		cin >> R1;
		cin >> R2;
		cout << "S" << i << "=" << RingS(R1, R2) << endl;
	}
	return 0;
}

#include<iostream>
int quarter(double x, double y) {
    if ((x > 0)and(y > 0)){
        return 1; 
    }
    if ((x < 0) and (y > 0)) { 
        return 2;
    }
    if ((x < 0) and (y < 0)) {
        return 3;
    }     
    if ((x > 0) and (y < 0)) {
        return 4;
    }
}

int main()
{
    using namespace std;
    int i;
    for (i = 1; i <= 3; ++i) {
        double x, y;
        cout << "X=" << endl;
        cin >> x;
        cout << "Y=" << endl;
        cin >> y;
        cout<< quarter(x, y);
    }
    return 0;
}
