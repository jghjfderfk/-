#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B;
	cout << "A=" << endl;
	cin >> A;
	cout << "B=" << endl;
	cin >> B;
	if (A > 2, B <= 3) {
		cout << "true";
	}
	else {
		cout << "false";
	}
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B, C;
	cout << "A=";
	cin >> A;
	cout << "B=";
	cin >> B;
	cout << "C=";
	cin >> C;
	if (B>A, C>B, C>A) {
		cout << "true";
	}
	else {
		cout << "false";
	}
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x;
	cout << "vvedite chislo" << endl;
	cin >> x;
	if (x >= 10, x < 100) {
		cout << "true";
	}
	else {
		cout << "false";
	}
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y, z, c;
	cout << "vvedite chislo" << endl;
	cin >> x;
	y = x % 10;
	z = (x % 100)/ 10;
	c = x / 100;
	if ((c > z, z > y) or (y > z, z > c)) {
		cout<<"true";
	}
	else {
		cout << "false";
	}
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y, z, c, s, P;
	cout << "vvedite  4-h znachnoe chislo" << endl;
	cin >> x;
	y = x / 100;
	z = x % 100;
	c = z % 10;
	s = z / 10;
	P = c * 10 + s;
	if (y == P) {
		cout << "true";
	}
	else {
		cout << "false";
	}
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int a, b, c;
	cout << "a=";
	cin >> a;
	cout << "b=";
	cin >> b;
	cout << "c=";
	cin >> c;
	if ((a * a == b * b + c * c) or (c * c == a * a + b * b) or (b * b == c * c + a * a)) {
		cout << "true";
	}
	else {
		cout << "false";
	}
	return 0;
}


#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int a, b, c;
	cout << "a=";
	cin >> a;
	cout << "b=";
	cin >> b;
	cout << "c=";
	cin >> c;
	if ((a < (b + c)) and (b < (a + c)) and (c < (b + a))) {
		cout << "true";
	}
	else {
		cout << "false";
	}
	return 0;
}
