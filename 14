#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "RUS");
	int A, B;
	cout << "Введите А и B (A<B)" << endl;
	cout << "A=" << endl;
	cin >> A;
	cout << "B=" << endl;
	cin >> B;
	for (int i = A; i <= B; i++) {
		for (int y = 1; y <= i; y++)
			cout << i;
	}
	return 0;
}

#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "RUS");
	int A, B, x;
	cout << "Введите А и B (A>B)" << endl;
	cout << "A=" << endl;
	cin >> A;
	cout << "B=" << endl;
	cin >> B;
	for (int i = 0; i <= A; i+=B) {
		x = i;
	}
	A = A - x;
	cout << A;
	return 0;
}

#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "RUS");
	int N, K=0, S=0;
	cout << "Введите N (N>1)" << endl;
	cout << "N=";
	cin >> N;
	while (S<=N) {
		K++;
		S += K;
	}
	cout << "Сумма =" << S << " " << "K=" << K;
	return 0;
}
