#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int a;
	double p;
	cin >> a;
	p = (double) a  / 180;
	cout << p << " radianov";
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	double p, a;
	cin >> p;
	a = (double) p * 180;
	cout << a << "  gradusov";
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, X;
	double C, Y;
	cout << "vvedite kol-vo konfet v kg" << endl;
	cin >> X;
	cout << "naznachte tseny" << endl;
	cin >> A;
	C = (double)A / X;
	cout << "togda tsena za 1 kg=" << C << "rubley"<<endl;
	cout << "vvedite kol-vo konfet v kg " << endl;
	cin >> Y;
	Y = Y * C;
	cout << "ih stoimost'=" << Y<<"rubley";
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int V1, V2, S;
	double t, S1, S2;
	cout << "vvedite skorost' 1-go" << endl;
	cin >> V1;
	cout << "vvedite skorost' 2-go" << endl;
	cin >> V2;
	cout << "vvedite rasstoyanie mejdy nimi" << endl;
	cin >> S;
	cout << "vvedite vremya kotoroe predstoit proehat'" << endl;
	cin >> t;
	S1 = (double) V1 * t;
	S2 = (double)V2 * t;
	S = S + S1 + S2;
	cout << "rasstoyanie mejdy nimi stanet=" << S;
	return 0;
}


#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B;
	double x ;
	cout << "vvedite A" << endl;
	cin >> A;
	cout << "vvedite B" << endl;
	cin >> B;
	cout << A << "*x+" << B << "=0" << endl;
	x = (double)-B / A;
	cout << "x=" << x;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A1, A2, B1, B2, C1, C2, d, d1, d2;
	double x, y;
	cout<< "A1*x + B1*y = C1" << endl;
	cout << "A2*x + B2*y = C2" << endl;
	cout << "vvedite A1" << endl;
	cin >> A1;
	cout << "vvedite A1" << endl;
	cin >> A2;
	cout << "vvedite A1" << endl;
	cin >> B1;
	cout << "vvedite A1" << endl;
	cin >> B2;
	cout << "vvedite A1" << endl;
	cin >> C1;
	cout << "vvedite A1" << endl;
	cin >> C2;
	cout << A1 << "*x+" << B1 << "*y=" << C1 << endl;
	cout << A2 << "*x+" << B2 << "*y=" << C2 << endl;
	d = A1 * B2 - A2 * B1;
	d1 = C1 * B2 - C2 * B1;
	d2 = A1 * C2 - A2 * C1;
	x =(double) d1 / d;
	y = (double)d2 / d;
	cout << "x=" << x << "    y=" << y;
		return 0;
}
