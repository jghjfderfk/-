#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B, t;
	cout << "Vvedite A" << endl;
	cin >> A;
	cout << "Vvedite B" << endl;
	cin >> B;
	t = A;
	A = B;
	B = t;
	cout << "A=" << A << endl;
	cout << "B=" << B << endl;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B, C, t;
	cout << "Vvedite A" << endl;
	cin >> A;
	cout << "Vvedite B" << endl;
	cin >> B;
	cout << "Vvedite C" << endl;
	cin >> C;
	t = A;
	A = B;
	B = C;
	C = t;
	cout << "A=" << A << endl;
	cout << "B=" << B << endl;
	cout << "C=" << C << endl;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B, C, t, f;
	cout << "Vvedite A" << endl;
	cin >> A;
	cout << "Vvedite B" << endl;
	cin >> B;
	cout << "Vvedite C" << endl;
	cin >> C;
	t = C;
	f = A;
	A = t;
	C = B;
	B = f;
	cout << "A=" << A << endl;
	cout << "B=" << B << endl;
	cout << "C=" << C << endl;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y;
	cout << "Vvedite x" << endl;
	cin >> x;
	y = 3 * pow(x, 6) - 6 * pow(x, 2) - 7;
	cout << "y=" << y;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y;
	cout << "Vvedite x" << endl;
	cin >> x;
	y = 4 * pow((x - 3), 6) - 7 * pow((x - 3), 3) + 2;
	cout << "y=" << y;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, A;
	cout << "Vvedite A" << endl;
	cin >> A;
	x = A * A;
	A = x * x;
	x = A * A;
	cout << "A^8=" << x;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, A, y;
	cout << "Vvedite A" << endl;
	cin >> A;
	x = A * A * A;
	y = x*x; 
	x = y*y*x;
	cout << "A^15=" << x;
	return 0;
}
