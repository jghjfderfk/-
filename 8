#include<iostream>
#include<math.h>#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y;
	cout << "vvedite chislo v byte" << endl;
	cin >> x;
	y = x / 1024;
	if (x % 1024 != 0) {
		y = y + 1;
	}
	cout << "chislo v kilobyte=" << y;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B, C;
	cout << "Vvedite A" << endl;
	cin >> A;
	cout << "Vvedite B" << endl;
	cin >> B;
	C = A / B;
	cout << C;
	return 0;
}


#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int A, B, C;
	cout << "Vvedite A" << endl;
	cin >> A;
	cout << "Vvedite B" << endl;
	cin >> B;
	C = A / B;
	C = A - C*B;
	cout << C;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y, p, c;
	cout << "Vvedite chislo" << endl;
	cin >> p;
	x = p % 10;
	y = p / 10;
	c = x * 10 + y;
	cout << c;
	return 0;
}

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int x, y, p, c;
	cout << "Vvedite chislo" << endl;
	cin >> p;
	x = p / 100;
	y = p % 100;
	c = y*10+x;
	cout << c;
	return 0;
}
